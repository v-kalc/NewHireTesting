{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "{{ Application tenant id }}",
    "ApplicationIdURI": "{{ Application Id URI }}",
    "ValidIssuers": "https://login.microsoftonline.com/TENANT_ID/v2.0,https://sts.windows.net/TENANT_ID/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": "{{ Telemetry instrumentation key }}",
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information"
    }
  },
  "App": {
    "AppBaseUri": "{{ Application base path }}",
    "TenantId": "{{ Application tenant id }}",
    "ConnectionName": "{{ Bot OAuth connection name }}",
    "ManifestId": "{{ Application end user manifest id }}",
    "HumanResourceTeamLink": "{{ Human resource Team link }}"
  },
  "Storage": {
    "ConnectionString": "{{ Storage Table connection string }}"
  },
  "SharePoint": {
    "SiteName": "{{ New employee onboarding site name}}",
    "NewHireCheckListName": "{{ New hire onboarding checklist }}",
    "SiteTenantName": "{{ SharePoint tenant name}}",
    "ShareFeedbackFormUrl": "{{ Survey MS forms URL }}",
    "CompleteLearningPlanUrl": "{{ Complete learning plan site URL }}",
    "NewHireQuestionListName": "{{ New hire introduction question site URL }}",
    "NewHireLearningPlansInWeeks": "{{ New hire learning plans in weeks }}"
  },
  "SecurityGroup": {
    "Id": "{{ Hiring manager/HR security group Id }}"
  },
  "i18n": {
    "DefaultCulture": "en-US",
    "SupportedCultures": "en-US"
  },
  "Cache": {
    "AuthorizationPolicyDurationInMinutes": 60
  },
  "MicrosoftAppId": "{{ Bot Id }}",
  "MicrosoftAppPassword": "{{ Bot Password }}",
  "PairUpBackgroundService": {
    "DelayInPairUpNotificationInDays": "{{ Pair-up notification interval }}",
    "NewHireRetentionPeriodInDays": "{{ New hire retention duration }}"
  }
}